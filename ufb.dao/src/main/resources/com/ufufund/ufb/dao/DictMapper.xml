<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ufufund.ufb.dao.DictMapper">

	<select id="getDictionary" resultType="com.ufufund.ufb.model.db.Dictionary">
  	 <![CDATA[
		SELECT
			A.PMST,
			A.PMKY,
			A.PMCO,
			A.PMNM,
			A.PMV1,
			A.PMV2,
			A.PMV3,
			A.PMV4,
			A.PMV5
			FROM
			SYS_DICTIONARY A
		ORDER BY A.PMKY
	 ]]>
  	</select>
  	
  	
  	
  	
</mapper>