<?xml version="1.0" encoding="UTF-8"?>
<layouts defaultdir="/WEB-INF/layouts/">

	<excludes>
		<!-- host匹配 -->
		<pattern>static.*</pattern>
		<pattern>api.*</pattern>
		<!-- 请求地址path匹配 -->
		<pattern>/**/*.ajax</pattern>
		<pattern>/**/*.service</pattern>
	</excludes>

	<layout page="layout/error.vm">
        <pattern>error/*</pattern>
    </layout>
    
    <layout page="layout/blank.vm">
        <pattern>uft/record_unit.vm</pattern>
        <pattern>uft/record_notice.vm</pattern>
    </layout> 
	
	<!-- family -->
	<layout page="layout/family.vm">
        <pattern>**/*.vm</pattern>
    </layout>
    
    <layout page="layout/blank.vm" default="true">
        <pattern>**/*.vm</pattern>
    </layout>
</layouts>
