<script>
$(function(){

	$("#container").removeClass("setting");

	// 表单提交时，验证	
	$("#next-step").on("click", function(){
		if(!check_pwd1($("#tradepwd"))){
			return false;
		}
		$("#form-autoTrade-result").attr("action","../family/autoTradePause_result.htm");
		$("#form-autoTrade-result").submit();
	});
	
	// 上一页
	$("#btn-autoTrade-pause").on("click",function(){
		$("#form-autoTrade-result").attr("action", "../family/autoTrade_pause.htm");
		$("#form-autoTrade-result").submit();
	});
});
</script>
