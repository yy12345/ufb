<style>

/*本页面的覆盖样式 payNotice.vm*/

.all-top-v2{
    margin-top: 40px;
}

.notice-content {
    margin-top: 30px;
    margin-bottom: 20px;
    background-color: transparent;
    padding-top: 0px;
    padding-bottom: 0px;
}

/*

.notice-content:last-child {
    margin-bottom: 60px;
}

*/

.item {
    border: 1px solid #ccc;
    background-color: #fff;
}

.item:last-child{
    border-top: none;
}

.fix-content {
    height: 50px;
    background-color: transparent;
    padding: 16px 10px;
    margin-top: 0px;
}

.item-title{
    margin-left: 17px;
    margin-top: 30px;
    margin-bottom: 10px;
}

.bottom-fix{
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    z-index: 100;
}

.bottom-fix-body{
    width: 935px;
    margin: 0 auto;
    border-radius: 2px;
    background-color: #e2e2e2;
    box-shadow: 5px 5px 5px #cfcfce;
    filter:alpha(opacity=90);
    -moz-opacity:0.9;
    opacity: 0.9;
}

.allselect{
    height: 50px;
    line-height: 50px;
    padding: 0 4px 0 21px;
}


/*本页面的覆盖样式 payReview.vm*/
.select-account-v2{
    background-color: transparent;
}
.select-account-body .li3-v2{
    float: left;
    margin-left: 150px;
    width:300px;
    margin-top:5px;
}
.select-account-body .li3-v2 p{
    margin:10px 0;
}
.select-account-body .paymoney{
    font-size: 18px;
    font-weight: bold;
    margin-left:3px;
}
.li2 p .cost-type {
    margin-right: 43px;
    cursor: pointer;
}
.grades-item-title{
    clear: both;
    background-color: #f6f6f6;
    width: 100%;
    margin: 15px auto 4px auto;
    height: 42px;
    line-height: 42px;
    color: #333;
    box-shadow: 3px 3px 3px #d5d5d5;
    position: relative;
    margin-left: -2px;
    cursor: pointer;
}
.grades-item-title .reduce{
    position: absolute;
    top: 1px;
    right: 20px;
    display: block;
    color: #666;
    font-size: 22px;
}
.grades-item-title label{
    margin-left: 6px;
    font-size: 16px;
    padding-left: 2em;
}
.affirm-organiz{
