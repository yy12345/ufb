<script type="text/javascript">
$(function(){
		$(".u2 .v2 .v2_1").click(function(){
            $(".u2 .v2").hide();
        });
		
		$(".btnA").on("click",function(){
			$("img").each(function(){
				if($(this).attr("load") == 0){
					alert("您提交的材料［" + $(this).parents(".v1").find(".v1_1").text() + "］不齐全，请确认，谢谢！");
					return false;
				};
			});
    		$("#form").attr("action", "../org/openAccoStep3.htm");
    		$("#form").submit();
    	});
		
		
 });
</script>
<script src="${staticDomain}/public/js/upload.js"></script>
