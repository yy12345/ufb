<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>com.ufufund</groupId>
		<artifactId>ufb.parent</artifactId>
		<version>1.0.0-SNAPSHOT</version>
	</parent>
	<artifactId>ufb-biz</artifactId>
	<groupId>com.ufufund.ufb</groupId>
	<packaging>jar</packaging>
	<name>${project.artifactId}</name>
	<version>${ufb.version}</version>
	<description>幼富宝 business module</description>

	<dependencies>
		<dependency>
			<groupId>com.ufufund.ufb</groupId>
			<artifactId>ufb-model</artifactId>
			<version>${ufb.version}</version>
		</dependency> 
		<dependency>
			<groupId>com.ufufund.ufb</groupId>
			<artifactId>ufb-common</artifactId>
			<version>${ufb.version}</version>
		</dependency>
		<dependency>
			<groupId>com.ufufund.ufb</groupId>
			<artifactId>ufb-dao</artifactId>
			<version>${ufb.version}</version>
		</dependency>
		<dependency>
			<groupId>com.ufufund.ufb</groupId>
			<artifactId>ufb-remote</artifactId>
			<version>${ufb.version}</version>
		</dependency>
		<dependency>
			<groupId>com.ufufund.ufb</groupId>
			<artifactId>ufb-service</artifactId>
			<version>${ufb.version}</version>
		</dependency>
		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-core</artifactId>
		</dependency>
		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-context</artifactId>
		</dependency>
        <dependency>
            <groupId>org.springframework</groupId>
            <artifactId>spring-aop</artifactId>
        </dependency>
        <dependency>
	        <groupId>org.aspectj</groupId>
	        <artifactId>aspectjweaver</artifactId>
	        <version>1.7.4</version>
	    </dependency>
	    <dependency>
			<groupId>org.quartz-scheduler</groupId>
			<artifactId>quartz</artifactId>
			<version>2.2.1</version>
		</dependency>
        <dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-test</artifactId>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<scope>test</scope>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
					<encoding>UTF-8</encoding>
					<!-- <compilerArgument>-proc:only</compilerArgument> -->
					<compilerArgument>-XDignore.symbol.file</compilerArgument>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>